#: Era_of_Music/Era_of_Music.cfg:4
msgid "Era of Music"
msgstr "Zeit der Musik"

#. [multiplayer_side]: id=strings, type=principal
#: Era_of_Music/Era_of_Music.cfg:12
msgid "Strings"
msgstr "Streicher"

#. [multiplayer_side]: id=woodwind, type=alto_sax
